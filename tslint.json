{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "functions": "never"
                }
            }
        ],
        "object-literal-sort-keys": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}
